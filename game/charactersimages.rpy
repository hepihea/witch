transform afternoon02_bg_showering_smoke_move:
    subpixel True
    xanchor 0.0
    easein_quad 30.0 xanchor 0.5

image night05_Cemetery_smoke_comp:
    contains:
        "fog_l_02"
        night05_Cemetery_fog_l_move01
    contains:
        "fog_l_01"
        night05_Cemetery_fog_l_move02

image night05_Cemetery_smoke_b_comp:
    contains:
        "fog_l_02"
        night05_Cemetery_fog_l_move01_b
    contains:
        "fog_l_01"
        night05_Cemetery_fog_l_move02_b
    

transform night05_Cemetery_fog_l_move01:
    subpixel True
    truecenter
    #additive 1.0
    zoom 1.0 xpos 1.5 ypos -0.125 
    parallel:
        linear 60.0 zoom 1.0 xpos -0.5 ypos -0.125 # Top Right Long Close
        linear 0.0 zoom 1.0 xpos 1.5 ypos -0.125 # Top Left Long Close
        repeat
        #linear 30.0 zoom 0.45 xpos 0.334 ypos 0.5 alpha 1.0 # Top Right Long Far
        #linear 0.0 zoom 0.45 xpos 0.667 ypos 0.5 # Top left Long Far
    parallel:
        easein_quad 20.0 alpha 1.0
        pause 20.0
        easeout_quad 18.0 alpha 0.0
        pause 2.0
        repeat

transform night05_Cemetery_fog_l_move01_b:
    subpixel True
    truecenter
    #additive 1.0
    zoom 1.0 xpos 1.5 ypos -0.125 
    parallel:
        linear 120.0 zoom 1.0 xpos -0.5 ypos -0.125 # Top Right Long Close
        linear 0.0 zoom 1.0 xpos 1.5 ypos -0.125 # Top Left Long Close
        repeat
        #linear 30.0 zoom 0.45 xpos 0.334 ypos 0.5 alpha 1.0 # Top Right Long Far
        #linear 0.0 zoom 0.45 xpos 0.667 ypos 0.5 # Top left Long Far
    parallel:
        easein_quad 40.0 alpha 1.0
        pause 40.0
        easeout_quad 36.0 alpha 0.0
        pause 4.0
        repeat

transform night05_Cemetery_fog_l_move02:
    subpixel True
    truecenter
    #additive 1.0
    zoom 1.0 xpos 1.5 ypos -0.125
    alpha 0.0
    parallel:
        pause 40.0
        linear 60.0 zoom 1.0 xpos -0.5 ypos 0.5 # Top Right Long Close # High a bit
        linear 0.0 zoom 1.0 xpos 1.5 ypos -0.125 # Top Left Long Close
        repeat
    parallel:
        pause 40.0
        easein_quad 20.0 alpha 1.0
        pause 20.0
        easeout_quad 18.0 alpha 0.0
