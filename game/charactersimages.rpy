label endupdate:
    
    window hide dissolve
    pause
    
    aj "HASTA AQUÍ LLEGA LA VERSIÓN [config.version]." with Dissolve (1.0)

    if Steam_check == False:
    
        aj "EL JUEGO SIEMPRE SERÁ GRATUITO,"

        aj "PERO EN MI {a=http://patreon.com/jonnymelabo}PATREON{/a}"

        extend " (activo desde Diciembre 2016),"
        
        aj "SIEMPRE ENCONTRARÁS LA ÚLTIMA VERSIÓN DISPONIBLE CON MESES DE ANTELACIÓN,"

        extend " POR SI OS SOBRA ALGÚN DOLAR... :)"
    
    aj "SI PUDIERAS DARME TU OPINIÓN SOBRE EL JUEGO,"

    extend " O UN \"Me gusta\" EN EL {a=https://www.facebook.com/jonnymelaboprod/}FACEBOOK{/a} DEL PROYECTO,"
    
    aj "TE LO AGRADECERÍA AÚN MÁS."

    extend " {size=50}Gracias{/size}."
    
