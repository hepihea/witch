    # pt "Dios..."
    pt "God..."

# game/day05_e_nightAfter_Elysium.rpy:998
translate english night05_NeusLastDate_FunicularLeaving_f3910c95:

    # n "A medida que os vais adentrando en ese largo pasillo,"
    n "As you make your way down the long corridor,"

# game/day05_e_nightAfter_Elysium.rpy:1000
