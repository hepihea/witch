# game/day05_e_nightAfter_Elysium.rpy:980
translate english night05_NeusLastDate_FunicularLeaving_b6712dd0:

    # n "A medida que os adentráis en ese oscuro pasadizo, los gritos se vuelven insoportables,"
    n "As you enter the dark passageway, the screams become unbearable,"

# game/day05_e_nightAfter_Elysium.rpy:982
