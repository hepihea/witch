    # ne "Shhh..."
    ne "Shhh..."

# game/day05_e_nightAfter_Elysium.rpy:970
translate english night05_NeusLastDate_FunicularLeaving_6d7b6795:

    # ne "No digas nada hasta que te lo diga."
    ne "Say nothing until I tell you."

# game/day05_e_nightAfter_Elysium.rpy:972
