    # p "..."
    p "..."

# game/day05_e_nightAfter_Elysium.rpy:976
translate english night05_NeusLastDate_FunicularLeaving_4d6504aa:

    # n "Puertas de madera putrefacta, más parecidas a las de un calabozo,"
